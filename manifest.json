{
  "name": "URL Redirect",
  "version": "1.0.0",
  "description": "Checks Word and redirects as required.",
 	"icons":{
    "128":"logo.png"
   },
  "background": { 
     "scripts": ["background.js"]
     },
   "manifest_version": 2,
   "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["content.js"]
   }
   ],
  "permissions": ["tabs"]
}
